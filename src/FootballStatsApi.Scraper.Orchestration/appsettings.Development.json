{
    "ConnectionStrings": {
        "AMQP": "amqp://guest:guest@localhost",
        "Football": "Server=127.0.0.1;User Id=postgres;Password=admin;Database=football;"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "System": "Debug",
                "Microsoft": "Debug"
            }
        },
        "WriteTo": [{
            "Name": "Console",
            "Args": {
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
                "outputTemplate": "[{Timestamp:HH:mm:ss.fff} {Level:u3}] {SourceContext} {Message:lj} {Properties:j} {Exception}{NewLine}"
            }
        }],
        "Enrich": ["WithExceptionDetails", "WithMemoryUsage", "WithEnvironment"]
    }
}